# Mailchimp newsletter integration
# Get your API key from: https://mailchimp.com/help/about-api-keys/
MAILCHIMP_API_KEY=

# Mapbox for maps/location features
# Get your token from: https://account.mapbox.com/access-tokens/
MAPBOX_ACCESS_TOKEN=

# Node.js version for deployment
NODE_VERSION=22

# Resend email service
# Get your API key from: https://resend.com/api-keys
RESEND_API_KEY=

# Cloudflare Turnstile (contact form bot protection)
# Get your keys from: https://dash.cloudflare.com/?to=/:account/turnstile
TURNSTILE_SITE_KEY=0x4AAAAAACHr1kbpPYRV4EtW
TURNSTILE_SECRET_KEY=